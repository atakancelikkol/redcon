{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "prefer-arrow-callback": "off",
        "max-len": "warn",
        "no-param-reassign": "warn",
        "no-unused-vars": "warn",
        "prefer-template": "warn",
        "no-undef": "warn",
        "no-use-before-define": "warn",
        "class-methods-use-this": "warn"
    }
}