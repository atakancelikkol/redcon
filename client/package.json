    "reconnecting-websocket": "^4.4.0",
    "sinon": "^9.0.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "^4.2.3",
    "@vue/test-utils": "^1.0.2",
    "babel-eslint": "^10.0.3",
